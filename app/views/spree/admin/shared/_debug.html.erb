<div id="debug-param" data-hook="debug_param">
<span class="glyphicon glyphicon-retweet btn-remove" title="Toggle Debug Param"></span>
  <%= debug(params) if Rails.env.development? %>
</div>
<script>
  $(document).ready(function() {
    $('.btn-remove').click(function() {
      $('.debug_dump').fadeToggle();
    });
  });
</script>