<% content_for :page_title do %>
  <%= link_to Spree.t(:navigations), spree.admin_navigations_url %> /
  <%= Spree.t(:new_navigation) %>
<% end %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @navigation } %>
<%= form_for [:admin, @navigation] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
